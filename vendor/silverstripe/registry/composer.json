{
    "name": "silverstripe/registry",
    "description": "Provide search and export interfaces for datasets.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sean Harvey",
            "email": "sean@silverstripe.com"
        },
        {
            "name": "Robert Curry",
            "email": "robert@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/cms": "^4.0",
        "silverstripe/admin": "^1.0",
        "silverstripe/assets": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Registry\\": "src/",
            "SilverStripe\\Registry\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

