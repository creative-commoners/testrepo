{
    "name": "silverstripe/reports",
    "type": "silverstripe-vendormodule",
    "description": "Reports module for SilverStripe CMS",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "cms",
        "reports"
    ],
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "http://silverstripe.com"
        },
        {
            "name": "The SilverStripe Community",
            "homepage": "http://silverstripe.org"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.5@dev",
        "silverstripe/admin": "^1.5@dev",
        "silverstripe/versioned": "^1.5@dev",
        "silverstripe/config": "^1.0@dev",
        "silverstripe/assets": "^1.5@dev",
        "silverstripe/vendor-plugin": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "expose": [
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Reports\\": "code/",
            "SilverStripe\\Reports\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs code/ tests/",
        "lint-clean": "phpcbf code/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
