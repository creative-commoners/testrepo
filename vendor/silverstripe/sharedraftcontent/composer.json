{
    "name": "silverstripe/sharedraftcontent",
    "type": "silverstripe-vendormodule",
    "description": "Share draft page content with non-CMS users",
    "license": "BSD-3-Clause",
    "keywords": ["silverstripe"],
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian.mooyman@gmail.com"
        },
        {
            "name": "Christopher Pitt",
            "email": "chris@silverstripe.com"
        },
        {
            "name": "David Craig",
            "email": "david@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.4@dev",
        "silverstripe/cms": "^4.4@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ShareDraftContent\\": "src/",
            "SilverStripe\\ShareDraftContent\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
