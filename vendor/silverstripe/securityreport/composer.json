{
    "name": "silverstripe/securityreport",
    "description": "Generates a security and access report for CMS users",
    "keywords": [
        "silverstripe",
        "security",
        "report"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4",
        "silverstripe/cms": "^4",
        "silverstripe/reports": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": [],
    "autoload": {
        "psr-4": {
            "SilverStripe\\SecurityReport\\": "src/",
            "SilverStripe\\SecurityReport\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
