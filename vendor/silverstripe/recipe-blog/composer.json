{
    "name": "silverstripe/recipe-blog",
    "type": "silverstripe-recipe",
    "description": "SilverStripe Blog Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "4.5.1@stable",
        "silverstripe/blog": "3.5.0@stable",
        "silverstripe/widgets": "2.0.2@stable",
        "silverstripe/content-widget": "2.1.0@stable",
        "silverstripe/spamprotection": "3.0.2@stable",
        "silverstripe/akismet": "4.0.3@stable",
        "silverstripe/comments": "3.3.0@stable",
        "silverstripe/comment-notifications": "2.0.1@stable",
        "colymba/gridfield-bulk-editing-tools": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}