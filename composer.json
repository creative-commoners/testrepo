{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/dynamodb": "^4",
        "cwp/cwp-recipe-kitchen-sink": "2.5.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "assets/*",
            "mysite/_config/*",
            ".env.example",
            "favicon.ico"
        ],
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "project-files-installed": [
            ".htaccess",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/blog.yml",
            "app/_config/contentblocks.yml",
            "app/_config/cwpadmin.yml",
            "app/_config/environmentcheck.yml",
            "app/_config/mysite.yml",
            "app/_config/security.yml",
            "app/_config/theme.yml",
            "app/_config/versionfeed.yml",
            "app/src/BlocksPage.php",
            "app/src/HomePage.php",
            "app/src/HomePageController.php",
            "app/src/Page.php",
            "app/src/PageController.php",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
