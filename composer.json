{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.x-dev",
        "silverstripe/recipe-form-building": "1.x-dev",
        "silverstripe-themes/simple": "~3.2.0",
        "silverstripe/admin": "dev-pulls/1/file-manager-icons as 1.x-dev",
        "silverstripe/asset-admin": "dev-pulls/1/file-manager-icons as 1.x-dev",
        "silverstripe/assets": "dev-pulls/1/file-manager-icons as 1.x-dev",
        "silverstripe/userforms": "dev-pulls/5/file-manager-icons as 5.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-asset-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-assets.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-userforms.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
