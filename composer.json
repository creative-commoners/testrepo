{
	"name": "cwp/cwp-installer",
	"description": "CWP Project Template",
	"require": {
		"cwp/cwp-recipe-basic": "1.1.0.x-dev",
		"cwp/cwp-recipe-blog": "1.1.0.x-dev",
		"cwp-themes/default": "~1.1.0@stable"
	},
	"require-dev": {
		"cwp/cwp-recipe-basic-dev": "~1.0.6@stable"
	},
	"suggest": {
		"silverstripe/textextraction": "Required for document text searching within CWP"
	},
	"config": {
		"process-timeout": 900
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cwp.govt.nz/"
		}
	],
	"minimum-stability": "dev"
}
